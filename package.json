{
  "name": "react-datasheet-ex",
  "version": "0.1.0",
  "description": "Custom renderers for react-datasheet",
  "author": "Patrick Jones <pjnola@gmail.com>",
  "main": "dist/main/index.js",
  "module": "dist/lib/index.js",
  "typings": "dist/types/index.d.ts",
  "repository": "https://github.com/patrick-jones/react-datasheet-ex.git",
  "license": "MIT",
  "keywords": [],
  "scripts": {
    "clean": "rimraf dist ./*.css",
    "lint": "tslint --project .",
    "fix": "tslint --fix --project .",
    "build": "run-s clean lint build:*",
    "build:cjs": "tsc -p tsconfig.commonjs.json",
    "build:es": "tsc -p tsconfig.module.json",
    "build:typedefs": "tsc --outDir ./dist/types --emitDeclarationOnly",
    "build:css": "cp src/react-datasheet-ex.css ./",
    "test": "jest --coverage --verbose",
    "watch": "jest --watch --coverage --verbose",
    "start": "start-storybook -p 3000 -c .storybook",
    "docs": "build-storybook -c .storybook -o dist/docs",
    "docs:publish": "run-s docs && gh-pages -m \"[ci skip] Updates\" -d dist/docs",
    "prepublishOnly": "run-s build test docs:publish",
    "prepare": "( [ ! -f ./dist/lib/index.js ] && npm install . && npm run build ) || echo \"SKIP: NON GIT SOURCE\"",
    "postpublish": "npm run clean"
  },
  "engines": {
    "node": ">=8.9"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dnd": "^5.0.0",
    "react-dnd-html5-backend": "^5.0.1",
    "react-dom": "^16.0.1",
    "react-datasheet": "^1.3.8"
  },
  "devDependencies": {
    "@dump247/storybook-state": "^1.5.0",
    "@storybook/addon-options": "^3.4.11",
    "@storybook/react": "^3.4.11",
    "@types/enzyme": "^3.1.14",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/enzyme-to-json": "^1.5.2",
    "@types/jest": "^23.3.2",
    "@types/memoize-one": "^3.1.1",
    "@types/node": "^10.11.3",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.8",
    "@types/storybook__addon-actions": "^3.4.1",
    "@types/storybook__addon-options": "^3.2.2",
    "@types/storybook__react": "^3.0.9",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "=23.6.0",
    "codecov": "^3.1.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.3.4",
    "gh-pages": "^2.0.0",
    "html-loader": "^0.5.5",
    "jest": "=23.6.0",
    "markdown-loader": "^4.0.0",
    "memoize-one": "^4.0.2",
    "npm-run-all": "^4.1.3",
    "opn-cli": "^3.1.0",
    "react": "^16.5.2",
    "react-datasheet": "^1.3.9",
    "react-dnd": "^5.0.0",
    "react-dnd-html5-backend": "^5.0.1",
    "react-dnd-test-backend": "^5.0.1",
    "react-dom": "^16.5.2",
    "rimraf": "^2.6.2",
    "storybook-readme": "^3.0.0",
    "ts-jest": "=23.10.1",
    "ts-loader": "^3.5.0",
    "tslint": "^5.10.0",
    "tslint-config-airbnb": "^5.11.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.1.1"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfig": "<rootDir>/tsconfig.test.json"
      }
    },
    "setupTestFrameworkScriptFile": "<rootDir>/src/setup-enzyme.ts",
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.(j|t)sx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/",
      "src/index.ts",
      "src/setup-enzyme.ts",
      "src/stories/"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  }
}
